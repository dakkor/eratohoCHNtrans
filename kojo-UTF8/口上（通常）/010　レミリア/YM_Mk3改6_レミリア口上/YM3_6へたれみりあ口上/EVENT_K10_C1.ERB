;-------------------------------------------------
;ビデオ撮影
;TEQUIP:50 == 0で解除口上
;-------------------------------------------------
@KOJO_10_TRAIN_MESSAGE_S_COM_100
;コマンド選択での撮影終了時
IF TEQUIP:50 == 0
	;PRINTFORMW 
;撮影開始時
ELSE
	;PRINTFORMW 
	;初めて
	IF CFLAG:8257 == 0
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORMW 「ふぅん、記録装置ね。悪趣味だこと」
			PRINTFORMW カメラのレンズを突きながら、ぼやくレミリア
			PRINTFORMW 「ご主人様以外に私の痴態を見られるなんて」
		;それ以外
		ELSE
			PRINTFORMW 「見慣れない機械ね」
			PRINTFORMW カメラの説明をしてやった。これでレミリアの艶姿を記録すると言うと、みるみる顔色を変える
		ENDIF
			CFLAG:8257 = 1
			RETURN 0
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1 && (CFLAG:8257 <= 2)
			PRINTFORMW 「そのビデオ、売る前に一緒に見てみない？」
			PRINTFORMW 「自分がどういう風によがってるのか、少し興味があるわ」
			;CFLAG:8257 = 3
		;それ以外
		ELSEIF CFLAG:8257 <= 1
			PRINTFORMW 「っ！」
			PRINTFORMW カメラを見つけると、レミリアはキッと%CALLNAME:PLAYER%を睨み付けた……
			CFLAG:8257 = 2
		ENDIF
			RETURN 0
	ENDIF
ENDIF

