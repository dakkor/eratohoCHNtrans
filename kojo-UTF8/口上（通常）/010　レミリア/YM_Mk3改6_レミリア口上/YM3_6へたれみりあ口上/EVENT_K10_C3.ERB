;-------------------------------------------------
;お風呂場プレイ
;TEQUIP:53 == 0で解除口上
;-------------------------------------------------
@KOJO_10_TRAIN_MESSAGE_S_COM_350
;解除時
IF TEQUIP:53 == 0
	;PRINTFORMW 
	;初めて
	IF CFLAG:8323 == 0
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORMW 「待ってて。髪を乾かすから」
		;それ以外
		ELSE
			PRINTFORMW 「お風呂場プレイなんて誰が考えたのかしら」
			PRINTFORMW 「この煮えたぎる思いをそいつにぶつけてやりたい」
		ENDIF
			CFLAG:8323 = 1
			RETURN 0
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1 && (CFLAG:8323 <= 2)
			PRINTFORMW 「ダメよ、ご主人様もちゃんと身体を拭きなさい」
			PRINTFORMW 「風邪なんてひいたら私を構ってくれる人がいなくなっちゃうじゃない」
			;CFLAG:8323 = 3
		;それ以外
		ELSEIF CFLAG:8323 <= 1
			PRINTFORMW 「お風呂に入ってこんなに疲れるなんて…」
			CFLAG:8323 = 2
		ENDIF
			RETURN 0
	ENDIF
;装着時
ELSE
	;PRINTFORMW 
	;初めて
	IF CFLAG:8322 == 0
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORMW 「あら、ご主人様も一緒に入ってくれるの？」
			PRINTFORMW 「楽しいお風呂になりそうね」
		;それ以外
		ELSE
			PRINTFORMW 「お風呂で？ほんと所構わずね…」
		ENDIF
			CFLAG:8322 = 1
			RETURN 0
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1 && (CFLAG:8322 <= 2)
			PRINTFORMW 「お風呂場で遊ぶメリットってどんなに汚してもすぐ洗えることよね」
			PRINTFORMW 「私も、ご主人様も、いっぱい出しても気にならないし、ね」
			;CFLAG:8322 = 3
		;それ以外
		ELSEIF CFLAG:8322 <= 1
			PRINTFORMW 「そんなに引っ張らなくても今行くわよ」
			CFLAG:8322 = 2
		ENDIF
			RETURN 0
	ENDIF
ENDIF

;-------------------------------------------------
;シャワー
;TEQUIP:54 == 0で解除口上
;-------------------------------------------------
@KOJO_10_TRAIN_MESSAGE_S_COM_351
;解除時
IF TEQUIP:54 == 0
	;PRINTFORMW 
;装着時
ELSE
	;PRINTFORMW 
	;初めて
	IF CFLAG:8314 == 0
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORML 「シャワーなんて別に平気よ。未熟な若造じゃないんだから」
			PRINTFORMW すらりとした胸を反らし、レミリアは自慢げに語った
			PRINTFORMW レミリアが嫌がるなら止めようかと思ったが、本人が平気だというなら遠慮は要らないようだ
		;それ以外
		ELSE
			PRINTFORML 「ふん、流水で吸血鬼が怯えるとでも思ってるの？五百の齢を舐めないでもらえるかしら」
			PRINTFORMW 流れる水は吸血鬼には毒かと思ったがそうでもないらしい
			PRINTFORMW それならば遠慮なくシャワーを使えるというものだ
		ENDIF
			CFLAG:8314 = 1
			RETURN 0
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1 && (CFLAG:8314 <= 2)
			PRINTFORMW 「ああ、気持ち良い」
			PRINTFORMW ぬるい湯を頭から被りながらレミリアは気分よさげに笑った
			CFLAG:8314 = 3
		;それ以外
		ELSEIF CFLAG:8314 <= 1
			PRINTFORMW 「ちょうどいいから身体を綺麗にしたいんだけど」
			PRINTFORMW シャワーから溢れる湯を手で受けながらレミリアがぼやいた
			CFLAG:8314 = 2
		ENDIF
			RETURN 0
	ENDIF
ENDIF

