@KOJO_10_TRAIN_MESSAGE_S_COM_352
;PRINTFORMW 
	;初めて
	IF CFLAG:8318 == 0
		;奉仕精神Lv3以上
		IF ABL:奉仕精神 >= 3
			PRINTFORMW 「こういうことはもっと肉付きの良い体じゃないと面白くないんじゃないの？」
			PRINTFORMW 「いいの？変な奴ね…」
		;それ以外
		ELSE
			PRINTFORMW 「石鹸はこういうことに使うものじゃないでしょ」
			PRINTFORMW 「くっ。やればいいんでしょ、やれば」
		ENDIF
			CFLAG:8318 = 1
			RETURN 0
	;二回目以降
	ELSE
		;恋慕＋奉仕精神Lv5
		IF TALENT:恋慕 == 1 && ABL:奉仕精神 >= 5 && (CFLAG:8318 <= 3)
			PRINTFORMW 「ああ、ごめんなさいご主人様。こんなちっちゃな体でごめんなさい」
			PRINTFORMW 「でも一生懸命するから、気持ち良くなって」
			PRINTFORMW ソープでぬめぬめになった身体を擦りつけながらレミリアが懇願する
			PRINTFORMW 同時に乳房や陰裂を擦り、その顔は段々と快楽に崩れていく
			;CFLAG:8318 = 4
		;奉仕精神Lv3以上
		ELSEIF ABL:奉仕精神 >= 3 && (CFLAG:8318 <= 2)
			PRINTFORMW 「こ、こんな貧相な体でいいの？」
			PRINTFORMW 「笑ったら殺すわよ！これでも気にして…」
			CFLAG:8318 = 3
		;それ以外
		ELSEIF  CFLAG:8318 <= 1
			PRINTFORMW 「気持ち良いの？…そう、変な奴」
			PRINTFORMW 「それにしても疲れるわね、これ」
			CFLAG:8318 = 2
		ENDIF
			RETURN 0
	ENDIF
