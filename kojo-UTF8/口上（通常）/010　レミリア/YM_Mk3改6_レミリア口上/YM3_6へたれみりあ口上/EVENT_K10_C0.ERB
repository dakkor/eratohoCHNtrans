;========================================================
@KOJO_10_TRAIN_MESSAGE_S_BEFORE
;眠ってる場合は戻る
SIF EQUIP:14
	RETURN 1
;口枷がある場合は戻る
SIF TEQUIP:42
	RETURN 1
;失神中・失神回復時は戻る
SIF TFLAG:898 == 1
	RETURN 1
;精神崩壊の場合は戻る
SIF TALENT:167
	RETURN 1

