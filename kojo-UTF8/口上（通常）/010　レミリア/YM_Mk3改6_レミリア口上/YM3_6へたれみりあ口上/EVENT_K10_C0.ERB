;-------------------------------------------------
;裸エプロン
;TEQUIP:59 == 0で解除口上
;-------------------------------------------------
@KOJO_10_TRAIN_MESSAGE_S_COM_90
;解除時
IF TEQUIP:59 == 0
	;PRINTFORMW 
	;初めて
	IF CFLAG:8325 == 0
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORMW 「あぁん、もう終わり？興が乗ってきたところだったのに」
			PRINTFORMW 仕舞われるエプロンを見て、レミリアは名残惜しげに呟いた
		;それ以外
		ELSE
			PRINTFORMW 「ああもうっ、顔から火が出そうとはこのことね！」
			PRINTFORMW 「恥ずかしくて人前でなんて絶対に出来ないわね！」
		ENDIF
			CFLAG:8325 = 1
			RETURN 0
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1 && (CFLAG:8325 <= 2)
			PRINTFORMW 「ふふっ、一時だけど良い夢が見れたわ」
			PRINTFORMW どことなく陰のある笑顔で、レミリアは呟いた
			CFLAG:8325 = 3
		;それ以外
		ELSEIF CFLAG:8325 <= 1
			PRINTFORMW 「はあ、やってる間よりも終わってからの方が恥ずかしいわね」
			PRINTFORMW ベッドにぺたんと腰掛けたレミリアは、顔をうつむけてそう呟いた
			CFLAG:8325 = 2
		ENDIF
			RETURN 0
	ENDIF
;装着時
ELSE
	;PRINTFORMW 
	;初めて
	IF CFLAG:8324 == 0
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORMW 「面白いものを出してきたわね」
			PRINTFORMW 「家事なんてできないけど、これを着てる間は旦那様とでも呼べばいいのかしら」
		;それ以外
		ELSE
			PRINTFORMW 「…これから犯そうって相手に着せるなんて趣味が悪すぎるわ」
			PRINTFORMW 「それとも何も考えてないのかしら」
		ENDIF
			CFLAG:8324 = 1
			RETURN 0
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1 && (CFLAG:8324 <= 2)
			PRINTFORMW 「旦那様、可愛い幼な妻に無理させないでくださいね」
			PRINTFORMW エプロンを身に着けると、ポーズを決めてみせるレミリア
			PRINTFORMW 言えば絶対に否定するだろうが、かなり乗り気の様子だ
			;CFLAG:8324 = 3
		;それ以外
		ELSEIF CFLAG:8324 <= 1
			PRINTFORMW 「…いいわ、着けてあげる」
			PRINTFORMW 「これ着てると、嫌に優しくなるし」
			;きつい調教をすると愛情経験が稼げないですからね。
			CFLAG:8324 = 2
		ENDIF
			RETURN 0
	ENDIF
ENDIF

