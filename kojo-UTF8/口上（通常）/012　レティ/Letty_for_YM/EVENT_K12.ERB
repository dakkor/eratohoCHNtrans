;ENDIF
;-------------------------------------------------
;初調教時 CFLAG5001
;-------------------------------------------------

IF CFLAG:5001 == 0
	DRAWLINE
	PRINTFORMW 「暗いし汚い部屋ね…。暑くないだけマシだけど」
	PRINTFORMW 「で、何をする気？ 楽しいことじゃないのは分かるけどね」
	PRINTFORMW レティは不審そうな顔でこちらを見ている
	CFLAG:5001 = 1
	RETURN 1

;-------------------------------------------------
;調教開始時（一回のみ） CFLAG5001
;-------------------------------------------------

;屈服刻印Lv1
ELSEIF CFLAG:5001 < 2 && MARK:屈服刻印 == 1 && TALENT:恋慕 == 0 && TALENT:服従 == 0
	DRAWLINE
	PRINTFORMW 「こんなことで思い通りになると思ってるの？」
	PRINTFORMW レティは敵愾心を隠すつもりもないようだ
	CFLAG:5001 = 2
	RETURN 1

;屈服刻印Lv2
ELSEIF CFLAG:5001 < 3 && MARK:屈服刻印 == 2 && TALENT:恋慕 == 0 && TALENT:服従 == 0
	DRAWLINE
	PRINTFORMW 「…何されたって心までは奪えないわ」
	PRINTFORMW 気丈な言葉を吐くものの、以前ほどの覇気はなく、%CALLNAME:MASTER%がにらみつけると意気消沈してしまった
	CFLAG:5001 = 3
	RETURN 1

;屈服刻印Lv3
ELSEIF CFLAG:5001 < 4 && MARK:屈服刻印 == 3 && TALENT:恋慕 == 0 && TALENT:服従 == 0
	DRAWLINE
	PRINTFORMW 「もう…諦めるしかないのかしら…」
	PRINTFORMW か細い声で呟くと、レティは%CALLNAME:MASTER%の目の前で服を脱いだ
	PRINTFORMW 白い肌はほのかに赤く色づいているが、目には涙を浮かべている
	CFLAG:5001 = 4
	RETURN 1

;恋慕
ELSEIF CFLAG:5001 < 5 && TALENT:恋慕 == 1
	DRAWLINE
	PRINTFORMW 部屋に入るや否や、レティがこちらに抱きついてきた。
	PRINTFORMW %CALLNAME:MASTER%は咄嗟に身構えたものの、どうやら様子がおかしい。
	PRINTFORMW 「私の負けよ」
	PRINTFORMW 「体がね…言うこと聞いてくれないの」
	PRINTFORMW レティの目線はこちらに真っ直ぐ向けられている。
	PRINTFORMW 「心でどれだけ拒絶しても、体があなたを欲しがってしまう」
	PRINTFORMW 「もう悩みたくないの。このまま流されたいの」
	PRINTFORMW 「お願い…。私をあなた一色で染めて。身も心も全部捧げるから…」
	PRINTFORMW そう言って%CALLNAME:MASTER%の頬を撫でるレティ
	PRINTFORMW 悲愴なまでの訴えを聞き入れた%CALLNAME:MASTER%は、レティの腰に手を回し、
	PRINTFORMW 懇願を承諾することを告げた
	PRINTFORMW 「ありがとう…大好きよ、%CALLNAME:MASTER%」
	CFLAG:5001 = 5
	RETURN 1

;淫乱+恋慕
ELSEIF CFLAG:5001 < 6 && TALENT:淫乱 && TALENT:恋慕 == 1
	DRAWLINE
	PRINTFORMW 「待ってたわ%CALLNAME:MASTER%」
	PRINTFORMW 「もう我慢できないの…。体が疼いてたまらないの」
	PRINTFORMW 「早く気持ちいいことして。溶けちゃうぐらい熱いのが欲しい…」
	PRINTFORMW 豹変した態度に面食らう%CALLNAME:MASTER%をよそに、レティはいそいそと服を脱いでいった
	CFLAG:5001 = 6
	RETURN 1

;助手の有無をチェック（いない場合は二回目以降へ飛ぶ）
ELSEIF ASSI < 0 
	CALL K12_KOJO2

;-------------------------------------------------
;簡易助手口上（Y1を助手のキャラ番号に置換、12をキャラ番号に変更する) CFLAG5002
;掛け合いをするキャラを増やす場合は、コピー＆ペーストをして、CFLAGの数を5003、5004と増やす
;-------------------------------------------------
;助手
;ELSEIF NO:ASSI == Y1
;	DRAWLINE
	;初めて
;	IF CFLAG:5002 == 0
		;恋慕＋恋慕時初口上（陥落イベント）が発生済み
;		IF TALENT:TARGET:恋慕 == 1 && CFLAG:201 >= 5
;			PRINTFORMW 
		;それ以外
;		ELSE
;			PRINTFORMW 
;		ENDIF
;			CFLAG:5002 = 1
;			RETURN 1
	;二回目以降
;	ELSE
		;恋慕
;		IF TALENT:TARGET:恋慕 == 1
;			PRINTFORMW 
		;それ以外
;		ELSE
;			PRINTFORMW 
;		ENDIF
;			RETURN 1
;	ENDIF

;ELSEIF NO:ASSI == Y1
;	DRAWLINE
	;初めて
;	IF CFLAG:5003 == 0
		;恋慕＋恋慕時初口上（陥落イベント）が発生済み
;		IF TALENT:TARGET:恋慕 == 1 && CFLAG:201 >= 5
;			PRINTFORMW 
		;それ以外
;		ELSE
;			PRINTFORMW 
;		ENDIF
;			CFLAG:5003 = 1
;			RETURN 1
	;二回目以降
;	ELSE
		;恋慕
;		IF TALENT:TARGET:恋慕 == 1
;			PRINTFORMW 
		;それ以外
;		ELSE
;			PRINTFORMW 
;		ENDIF
;			RETURN 1
;	ENDIF
;口上のある助手が居ない場合は、通常の二回目以降の口上へ飛ぶ
ELSE
	CALL K12_KOJO2
ENDIF


;-------------------------------------------------
;調教開始時（2回目以降、12をキャラ番号に変更する） CFLAGなし 
;-------------------------------------------------
@K12_KOJO2
;反発刻印Lv3
IF MARK:9 == 3
	DRAWLINE
	PRINTFORMW 「寒気さえあればねぇ。すぐ息の根をとめてあげるのに」
	PRINTFORMW 刺すような視線に、%CALLNAME:MASTER%は別の意味で寒気を覚えた
	RETURN 1

;屈服刻印Lv0
ELSEIF MARK:2 == 0
	DRAWLINE
	PRINTFORMW 「好きでもない男に触れられて、女が喘ぐとでも？」
	PRINTFORMW 視線すら合わせず、レティは冷徹に言い放った
	RETURN 1

;屈服刻印Lv1
ELSEIF MARK:2 == 1
	DRAWLINE
	PRINTFORMW 「するならさっさとして。そしてさっさとどこかに行って」
	PRINTFORMW 下手クソと言わんばかりに、軽蔑した表情が%CALLNAME:MASTER%に向けられた
	RETURN 1

;屈服刻印Lv2
ELSEIF MARK:2 == 2
	DRAWLINE
	PRINTFORMW 「お願い、これ以上触らないで…」
	PRINTFORMW わずかに抵抗するレティを押さえつけ、乱暴に服を脱がせていった
	RETURN 1

;屈服刻印Lv3
ELSEIF MARK:2 == 3 && TALENT:恋慕 == 0
	DRAWLINE
	PRINTFORMW 「今日もするのね…。わかったわ」
	PRINTFORMW 「その…、酷いことはしないでね」
	PRINTFORMW もはや諦めたのか、自ら衣服を脱ぎ捨てていくレティ
	PRINTFORMW その吐息はわずかに荒くなっていた
	RETURN 1

;淫乱+恋慕
ELSEIF TALENT:淫乱 && TALENT:恋慕 == 1
	DRAWLINE
	PRINTFORMW 「いやらしい女になったものだと自分でも思うわ」
	PRINTFORMW 「でもいいの…もう%CALLNAME:MASTER%とエッチすることしか考えられない」
	PRINTFORMW 「今日もいっぱい犯して…ぐちょぐちょにして欲しいの」
	PRINTFORMW ベッドに押し倒されたレティの表情は、快楽に支配された牝そのものだった
	RETURN 1

;恋慕
ELSEIF TALENT:恋慕 == 1
	DRAWLINE
	;ランダムで口上が変化する（使わない場合はすべて同じにすればよい）
	IF RAND:3 == 0
		PRINTFORMW 「今日はどんなことされちゃうのかしら。ふふふ」
		PRINTFORMW 柔和な笑みとともにレティは服を脱ぎ捨てた
		PRINTFORMW 「いっぱい気持ちよくしてね、%CALLNAME:MASTER%」
	ELSEIF RAND:2 == 0
		PRINTFORMW 「来てくれたのね。待ってたわ」
		PRINTFORMW 悩ましくベッドに寝そべりながら、嬉しそうに手招きするレティ
		PRINTFORMW 「たまにはあなたに脱がせてほしいの…」
		PRINTFORMW 「ん…がっつかないで。私はどこにも逃げないわ」
	ELSE
		PRINTFORMW 「あ、ちょっと待っててね」
		PRINTFORMW 部屋に入ると、レティが丁寧に髪を乾かしていた。シャワーでも浴びていたのだろう
		PRINTFORMW 手伝って欲しいと言うので、乾いた所から順に櫛で梳いてやる
		PRINTFORMW 「…うん、うん。よし、綺麗になったわ」
		PRINTFORMW 鏡の前でひとしきり確認すると、レティはこちらを見てにっこり微笑んだ
		PRINTFORMW 「待たせちゃってごめんなさい。それじゃ始めましょ♪」
	ENDIF
	RETURN 1
ENDIF
RETURN 0

;-------------------------------------------------
;調教終了時のセリフ
;-------------------------------------------------
@KOJO_12_SLAVE_MESSAGE_TRAIN_END
;眠ってる場合は戻る
SIF EQUIP:14
	RETURN 0
;口枷がある場合も戻る
SIF TEQUIP:42
	RETURN 0
;失神中・失神回復時も戻る
SIF TFLAG:899 >= 1 || TFLAG:898 == 1
	RETURN 0
;--------------------------------------------------
;特定助手、主人時に特殊助手口上を呼び出す(Y1を特定助手、12を主人のキャラ番号に置換）
;--------------------------------------------------
;IF NO:MASTER == Y1
;	CALL LES_EVENTEND_K12_MY1
;	RETURN 0
;ENDIF
;IF ASSI > 0 && ASSIPLAY
;	SIF NO:ASSI == Y1
;	CALL LES_EVENTEND_K12_MY1
;	RETURN 0
;ENDIF
;--------------------------------------------------
;調教終了時のセリフ
;--------------------------------------------------
;反発刻印Lv3
IF MARK:9 == 3
	DRAWLINE
	PRINTFORMW 「力が発揮出来ないのが悔しいわ」
	PRINTFORMW 「今なら背中がガラ空きなのに…」
	PRINTFORMW 物騒な言葉を聞き流しながら、部屋のドアを強めに閉めた
	RETURN 1

;屈服刻印Lv1以下
ELSEIF MARK:2 <= 1
	DRAWLINE
	PRINTFORMW 「さっさと出て行って。臭いわ」
	PRINTFORMW 「せめてもう少し上手くなってから来ることね」
	PRINTFORMW レティは無表情なまま、淡々と服を着なおした
	RETURN 1

;屈服刻印Lv2
ELSEIF MARK:2 == 2
	DRAWLINE
	PRINTFORMW 「はぁ…ん、はぁ…」
	PRINTFORMW 荒く息をつきながらも、レティの目は屈した奴隷のそれではない
	RETURN 1

;屈服刻印Lv3
ELSEIF MARK:2 == 3 && TALENT:恋慕 == 0
	DRAWLINE
	PRINTFORMW 「ダメ…。流されちゃだめなの…」
	PRINTFORMW 服を着ることもせず、ベッドにだらりと体を預けながら独り言を呟いている
	PRINTFORMW 熱っぽく%CALLNAME:MASTER%を見つめる瞳には、もはや抵抗の光は見えない
	RETURN 1

;恋慕＋体力500未満
ELSEIF TALENT:恋慕 == 1 && BASE:0 < 500
	DRAWLINE
	PRINTFORMW 「ごめんね。ちょっと疲れちゃったかも…」
	PRINTFORMW そう言って静かに眠りに落ちるレティの髪を、%CALLNAME:MASTER%は優しく撫でた。
	RETURN 1

;恋慕
ELSEIF TALENT:恋慕 == 1
	DRAWLINE
	PRINTFORMW 「まだ大丈夫だけど、あなたの言うことなら従うわ」
	PRINTFORMW 「近いうちにまた来てね？ 約束よ？」
	RETURN 1

;淫乱+恋慕
ELSEIF TALENT:淫乱 && TALENT:恋慕 == 1
	DRAWLINE
	PRINTFORMW 「いやぁ…もっと、もっとしてよ…」
	PRINTFORMW 「足りないの…。いやらしいこともっとしたい…」
	PRINTFORMW 切なげな声を背に、%CALLNAME:MASTER%は部屋のドアに手をかけた
	RETURN 1
ENDIF
RETURN 0

;--------------------------------------------------
;@KOJO_MESSAGE_COM関係（12をキャラ番号に置換）
;コマンド実行時に出力されます
;--------------------------------------------------
;========================================================
;コマンド口上前の共通関数
;特定の条件下でコマンド口上を表示させないようにする場合はRETURN 1
;1以外を返した場合はコマンド口上は表示される
;========================================================
@KOJO_12_TRAIN_MESSAGE_S_BEFORE

@KOJO_12_TRAIN_MESSAGE_S_GENERAL
;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
;SIF ASSI > 0 && ASSIPLAY
;	RETURN 0 
;眠ってる場合は戻る
SIF EQUIP:14
	RETURN 0
;口枷があり、口枷をつけるコマンドでない場合も戻る
SIF (TEQUIP:42 && (SELECTCOM != 226 && SELECTCOM != 229 && SELECTCOM != 506 && SELECTCOM != 509 && SELECTCOM != 583))
	RETURN 0
;失神中・失神回復時も戻る
SIF TFLAG:899 >= 1 || TFLAG:898 == 1
	RETURN 0
;--------------------------------------------------
;特定助手、主人時に特殊助手口上を呼び出す(Y1を特定助手、12を主人のキャラ番号に置換）
