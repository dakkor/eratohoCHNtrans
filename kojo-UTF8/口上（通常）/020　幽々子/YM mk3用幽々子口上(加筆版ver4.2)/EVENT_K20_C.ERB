@KOJO_20_TRAIN_MESSAGE_S_COM_0

;初めて
IF CFLAG:9000 == 0
	;処女
	IF TALENT:0 == 1
		PRINTFORMW 「んっや……いやっ！な、何してるのよ！」
	;それ以外
	ELSE
		PRINTFORMW 「……やめてよ、気持ち悪い」
	ENDIF
		CFLAG:9000 = 1
		RETURN 0
;二回目以降
ELSE
	;恋慕
	IF TALENT:150 == 1
		PRINTFORMW 「んっうんっ……あっ……気持ちいい……」
		PRINTFORMW 幽々子は愛撫への快感を存分に楽しんでいる
		CFLAG:9000 = 5
	;屈服刻印Lv3
	ELSEIF MARK:2 == 3
		PRINTFORMW 「ん……あん……ああっ……」
		PRINTFORMW 幽々子を優しく愛撫すると時折嬌声を漏らした
		CFLAG:9000 = 4
	;屈服刻印Lv2
	ELSEIF MARK:2 == 2
		PRINTFORMW 「ん……あっ……くっ……」
		PRINTFORMW 幽々子は愛撫への快感を堪えている
		CFLAG:9000 = 3
	;それ以外
	ELSEIF MARK:2 <= 1
		PRINTFORMW 「……やめてくれないかしら、気持ち悪い」
		CFLAG:9000 = 2
	ENDIF
		RETURN 0
ENDIF

