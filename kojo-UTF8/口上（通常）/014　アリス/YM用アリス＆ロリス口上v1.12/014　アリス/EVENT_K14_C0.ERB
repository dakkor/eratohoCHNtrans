;-------------------------------------------------
;愛撫
;-------------------------------------------------
@KOJO_14_TRAIN_MESSAGE_S_COM_0
;初回実行時
IF TFLAG:95 == 1
	IF CFLAG:7000 == 1
		IF TALENT:恋慕 == 1
			PRINTFORMW 「良いよ、触って…」
		ELSE
			PRINTFORMW 「やッ…、は、うん……」
		ENDIF
	ELSE
		IF TALENT:恋慕 == 1
			PRINTFORMW 「はあっ…、気持ち良い…」
		ELSE
			PRINTFORMW 「やだ、触らないで…！」
		ENDIF
	ENDIF
;2回目以降
ELSE
	IF CFLAG:7000 == 1
		IF TALENT:恋慕 == 1
			IF RAND:3 == 0
				PRINTFORMW 「んっ、%MASTERNAME%の手、あったかい…」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「ああっ、%MASTERNAME%っ、%MASTERNAME%っ！」
			ELSE
				CALL K14_GASP
			ENDIF
		ELSE
			IF RAND:2 == 0
