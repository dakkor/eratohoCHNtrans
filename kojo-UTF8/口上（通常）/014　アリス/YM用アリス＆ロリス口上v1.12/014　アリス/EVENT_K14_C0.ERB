			ELSE
				CALL K14_GASP
			ENDIF
		ENDIF
	ELSE
		IF TALENT:恋慕 == 1
			IF RAND:3 == 0
				PRINTFORMW 「もっと触って…、好きなだけ…」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「もうっ、相変わらず手つきがいやらしいんだから…」
			ELSE
				CALL K14_GASP
			ENDIF
		ELSE
			IF RAND:2 == 0
