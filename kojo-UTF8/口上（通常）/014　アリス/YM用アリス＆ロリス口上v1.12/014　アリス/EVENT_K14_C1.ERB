@KOJO_14_TRAIN_MESSAGE_S_COM_100
;コマンド選択での撮影終了時
IF TEQUIP:50 == 0
	;PRINTFORMW 
;撮影開始時
ELSE
	;初回時
	IF TFLAG:95 == 1
