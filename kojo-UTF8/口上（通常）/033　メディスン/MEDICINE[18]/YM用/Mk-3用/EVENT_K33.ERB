;=================================================
;1100:売春宿で指名された時（お昼イベント）
;=================================================
@KOJO_33_MESSAGE_SUB_EVENT_SPR_SCENE1100
;共通処理呼び出し
CALL SELF_KOJO_MEDI_K33, 1100

;まだ娼館デビュー前
IF EXP:76 == 0
	WAIT
	PRINTFORMW 「えっ？　わ、私……！？」
	PRINTFORMW 「うぅぅ～～。ど、どうしよう……ちょっと怖くなってきちゃったよぅ……」
	;恋慕
	SIF TALENT:150
		PRINTFORMW 「で、でも……%TSTR:300%の為なんだもの。がんばらなくちゃ……！」
;デビュー後
ELSE
	PRINTW  
	;娼婦かつ、対多人数の時
	IF TALENT:260 && (CFLAG:1501 == 5 || CFLAG:1501 == 6)
		IF RAND:3 == 0
			PRINTFORMW 「お客さんがこんなにいっぱい……」
			PRINTFORML 「私がみんなのこと気持ち良くしてあげるのかな？
			PRINTFORMW 　みんなが私のこと気持ち良くしてくれるのかな？」
			PRINTFORMW 「あん、考えただけでドキドキしてきちゃった……♪」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「うわぁ～、お客さんいっぱいだね」
			PRINTFORMW 「頑張り甲斐がありそう」
		ELSE
			PRINTFORMW 「今日もいっぱい来てくれてるね」
			PRINTFORMW 「ふふ、みんな私が目当てなのかな？」
		ENDIF
	;娼婦
	ELSEIF TALENT:260
		;常連客が居る＋顧客の子を妊娠＋グッド妊娠
		IF TALENT:140 && TALENT:265 && CFLAG:76 == 1 && CFLAG:5210 == 1 && RAND:2 == 0
			;求愛依存
			IF TALENT:261
				PRINTFORML 「いらっしゃい♪」
				PRINTFORMW 「ねぇねぇ、見てみて。ほら私……赤ちゃん出来ちゃった」
				PRINTFORMW 「もしかしたら、お客さんのかもしれないよ？　……ふふ♪」
			;それ以外
			ELSE
				PRINTFORMW 「あ、いらっしゃいお客さん」
				PRINTFORML 「ねぇ、私……赤ちゃん出来ちゃったみたい」
				PRINTFORMW 「もし、お客さんのだったら……どうする？」
			ENDIF
		;常連客が居る
		ELSEIF TALENT:265 && RAND:4 == 0
			PRINTFORML 「はーい。……あっ、また来てくれたんだ」
			PRINTFORMW 「私の事、そんなに気に入ってくれたの？」
			PRINTFORMW 「じゃあ、今日もいっぱいサービスしてあげるからね♪」
		ELSEIF RAND:3 == 0
			;メディ以外の奴隷が娼館に居る(5分の3)
			IF FLAG:3001 > 1 && RAND:5 < 3
				;娼館に居る他のキャラを探す
				CALL K33_SYOUKANNO_OTOMODACHI, TARGET
				;RESULTが-1だったり、キャラ数を超えていたら他の口上分岐へ(念の為の処理)
				SIF RESULT < 0 || RESULT >= CHARANUM
					GOTO HENKOU
				PRINTFORML 「え？　あ、私？」
				PRINTFORMW メディスンは%CALLNAME:RESULT%とお喋りをしていたようだ
				PRINTFORML 「呼ばれちゃったから行かないと」
				PRINTFORMW 「じゃあ%CALLNAME:RESULT%、また後でね。%CALLNAME:RESULT%も頑張ってね」
			;それ以外
			ELSE
				$HENKOU
				PRINTFORMW 「よし！　今日も頑張って、お客さんに喜んで貰おうっと♪」
				;恋慕
				SIF TALENT:150
					PRINTFORMW 「そうすれば、%TSTR:300%にも喜んで貰えるもの♪」
			ENDIF
		ELSEIF RAND:2 == 0
			PRINTFORMW 「はーい。ふふ、お客さん、いらっしゃい」
			PRINTFORMW 「早くお部屋行こ♪」
		ELSE
			PRINTFORMW 「選んでくれてありがと。いっぱいサービスしてあげるね♪」
		ENDIF
	;従順3以上
	ELSEIF ABL:10 >= 3
		IF RAND:4 == 0
			PRINTFORML 「い、いらっしゃいませ」
			PRINTFORMW 「え～と、ご指名ありがとうございます……だったかな？」
		ELSEIF RAND:3 == 0
			PRINTFORML 「あ……い、いらっしゃい、お客さん」
			PRINTFORMW 「ほんとに、私でいいの……？」
		ELSEIF RAND:2 == 0
			PRINTFORML 「今日はどんな人なのかな？」
			PRINTFORMW 「優しくしてくれる人だと良いんだけど……」
		ELSE
			PRINTFORMW 「うん、分かった。今行くわ」
		ENDIF
	;それ以外
	ELSE
		IF RAND:2 == 0
			PRINTFORML 「またわざわざ私みたいな“人形”を選ぶなんて……」
			PRINTFORMW 「物好きな人って結構居るのね」
		ELSE
			;メディ以外の奴隷が娼館に居る
			IF FLAG:3001 > 1
				PRINTFORML 「えっ、嘘……私なの？」
				PRINTFORMW 「うぅ～～、やだよぅ……」
			;娼館に居るのがメディだけ
			ELSE
				PRINTFORML 「また、知らないような人とエッチな事しなきゃならないの……？」
				PRINTFORMW 「もうやだ、帰りたいよぅ……スーさん」
			ENDIF
		ENDIF
	ENDIF
	PRINTL 
	CFLAG:8198 |= 2
ENDIF


