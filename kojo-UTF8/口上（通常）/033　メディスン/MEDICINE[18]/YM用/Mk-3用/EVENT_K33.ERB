	;従順Lv5＋屈服刻印Lv3＋反発刻印無し
	ELSEIF ABL:10 >= 5 && MARK:2 >= 3 && MARK:9 == 0
		;お医者プレイ中
		IF TEQUIP:57
			PRINTFORML 「ただのお医者さんごっこかと思ってたのに」
			PRINTFORMW 「ほんとに産むところまでやらなきゃダメなの……？」
		ELSE
			PRINTFORMW 「…………やっぱりやだよぉ。……うぅ、スーさぁん」
		ENDIF
		CFLAG:6066 = 3
	;それ以外
	ELSE
		;お医者プレイ中
		IF TEQUIP:57
			PRINTFORML 「へ、変なもの飲ませたと思ったら……っ」
			PRINTFORMW 「だ、誰があなたの赤ちゃんなんかっっ！」
		ELSE
			PRINTFORMW 「いやぁ、やだぁ！　私、お母さんになんかなりたくないっ！！」
		ENDIF
		CFLAG:6066 = 2
	ENDIF
