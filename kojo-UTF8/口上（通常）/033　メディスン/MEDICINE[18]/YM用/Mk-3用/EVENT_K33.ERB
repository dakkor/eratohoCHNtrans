ELSE
	;恋慕
	IF TALENT:150
		;お医者プレイ中
		IF TEQUIP:57
			PRINTFORML 「“ふにんちりょう”っていうやつ？」
			PRINTFORMW 「えへへ、これで\@(CFLAG:5200 > 0) ? また # ようやく\@赤ちゃんできるかな？」
			PRINTFORMW 「お願いします、%TSTR:300%♪」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「あんまり美味しくないお薬だったけど、我慢してちゃんと飲んだよ？」
			PRINTFORMW 「早くご褒美頂戴♪」
		ELSEIF RAND:2 == 0
			PRINTFORML 「このお薬を飲ませるってことは……」
			PRINTFORMW 「んふふ、いいよ……%TSTR:300%。私の中に%TSTR:300%の赤ちゃん頂戴♪」
			PRINTFORMW そう言うと、%CALLNAME:MASTER%の胸に身体を預けスッと力を抜いた……
		ELSE
			PRINTFORMW 「赤ちゃん出来るかなぁ？」
			PRINTFORMW 「ふふっ、ほら早く、%TSTR:300%。一緒にがんばりましょう♪」
		ENDIF
		CFLAG:6066 = 5
	;淫乱・烙印・服従・隷属のうち、どれか１つでもある
	ELSEIF TALENT:160 || TALENT:161 || TALENT:162 || TALENT:170
		;お医者プレイ中
		IF TEQUIP:57
