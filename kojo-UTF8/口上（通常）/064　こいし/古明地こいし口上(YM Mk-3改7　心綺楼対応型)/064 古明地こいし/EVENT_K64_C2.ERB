;3=奴隷しか着脱できない
;-------------------------------------------------
@K64_MESSAGE_COM_290
SELECTCASE TFLAG:99
	CASE 0
		;解除
		IF TEQUIP:PLAYER:31 == 0
			;PRINTFORMW 
		;装着時
		ELSE
			;PRINTFORMW 
		ENDIF
	CASE 1
		;解除
		IF TEQUIP:31 == 0
			;PRINTFORMW 
		;装着時
		ELSE
			;PRINTFORMW 
		ENDIF
	CASE 2
		;解除
		IF TEQUIP:PLAYER:31 == 0
			;PRINTFORMW 
		;装着時
		ELSE
			;PRINTFORMW 
		ENDIF
	CASE 3
		;解除
		IF TEQUIP:31 == 0
			;PRINTFORMW 
		;装着時
		ELSE
			;PRINTFORMW 
		ENDIF
ENDSELECT

