		PRINTFORMW 素質変化イベントをONにしました
		CFLAG:8001 = 1
	;いいえ
	ELSEIF RESULT == 1
		PRINTFORMW 口上内での素質変化はしません
		PRINTFORMW このままゲームを開始します
		CFLAG:8001 = 0
	ELSE
		GOTO INPUT_LOOP_0
	ENDIF
	;素質変化イベントONのときは続く
	IF CFLAG:8001 == 1
		;幽霊同士の相性上昇
		PRINTL 白玉楼の人びととの関係は…
		PRINTL [0] 宴会だけでなくいろんなことも…
		PRINTL [1] ビジネスライク
		$INPUT_LOOP_1
		INPUT
		;Ｏ
		IF RESULT == 0
			PRINTFORMW リリカと幽々子、妖夢の相性を上昇させます
			RELATION:19 = 120
			RELATION:20 = 120
		;１
		ELSEIF RESULT == 1
			PRINTFORMW 相性の変更は行ないません
		ELSE
			GOTO INPUT_LOOP_1
		ENDIF
		PRINTFORML 
		;ふたなり
		PRINTL 最初からふたなりにすることができますがどうしますか？
		PRINTL [0] ふたなりリリカちゃんばんざい！！
		PRINTL [1] 後で生やすのが正解だろ、常識的に考えて…
		PRINTL [2] そのような嗜好は理解できない
		PRINTL [3] いっそのこと男の娘に…
		$INPUT_LOOP_2
		INPUT
		;はやす
		IF RESULT == 0
			PRINTFORMW ふたなりにします
			TALENT:ふたなり = 1
		;生やさない
		ELSEIF RESULT == 1
			PRINTFORMW [不思議な根]を付与します
			TALENT:不思議な根 = 1
		ELSEIF RESULT == 2
			PRINTFORMW ふたなりにはしません
		ELSEIF  RESULT == 3
			PRINTFORMW [オトコ]にします
			TALENT:オトコ = 1
		ELSE
			GOTO INPUT_LOOP_2
		ENDIF
	ENDIF
