;-------------------------------------------------
;コンドーム
;TEQUIP:31 == 0で解除口上
;TFLAG:99により着脱者と状況が分岐
;0=調教者奴隷双方が選択できるかつ調教者が着脱
;1=調教者奴隷双方が選択できるかつ奴隷が着脱
;2=調教者しか着脱できない
;3=奴隷しか着脱できない
;-------------------------------------------------
@KOJO_27_TRAIN_MESSAGE_S_COM_290
SELECTCASE TFLAG:99
	CASE 0
		;解除
		IF TEQUIP:PLAYER:31 == 0
			;PRINTFORMW 
		;装着時
		ELSE
			;PRINTFORMW 
		ENDIF
	CASE 1
		;解除
		IF TEQUIP:31 == 0
			;PRINTFORMW 
		;装着時
		ELSE
			;PRINTFORMW 
		ENDIF
	CASE 2
		;解除
		IF TEQUIP:PLAYER:31 == 0
			;PRINTFORMW 
		;装着時
		ELSE
			;PRINTFORMW 
		ENDIF
	CASE 3
		;解除
		IF TEQUIP:31 == 0
			;PRINTFORMW 
		;装着時
		ELSE
			;PRINTFORMW 
		ENDIF
ENDSELECT

