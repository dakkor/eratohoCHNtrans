;-------------------------------------------------
;二人フェラチオ
;奴隷と助手が主人の竿に奉仕する
;-------------------------------------------------
@KOJO_2_TRAIN_MESSAGE_S_COM_303
;PRINTFORMW 
	IF TFLAG:95 == 1
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORMW 「２人でお前のおちんちんを同時に舐めるのか」
			PRINTFORMW 「よし、%CALLNAME:ASSI%には負けないぜ？」
		;その他の場合
		ELSE
			PRINTFORMW 「%CALLNAME:ASSI%と２人でやるのか？」
			PRINTFORMW 「……まぁ、舐める部分が半分になるなら、普通のフェラよりはまだマシだな」
		ENDIF
		RETURN 0
	;２回目以降
	ELSE
