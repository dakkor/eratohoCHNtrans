;-------------------------------------------------;
@KOJO_87_EXTRA_INITIALIZATION
IF CFLAG:5087 == 0
	PRINTL 処女モードに移行しますか？
	PRINTL [0] はい
	PRINTL [1] いいえ
	$INPUT_LOOP
	INPUT
	;はい
	IF RESULT == 0
		PRINTFORMW ―処女モードに素質、能力を変更します―
		TALENT:0 = 1
		EXP:Ｖ経験 = 0
		ABL:Ｖ感覚 = 0
		CFLAG:5087 = 1
	;いいえ
	ELSEIF RESULT == 1
		PRINTFORMW ―通常モードで調教を開始します―
		CFLAG:5087 = 2
	ELSE
		GOTO INPUT_LOOP
	ENDIF
ENDIF

