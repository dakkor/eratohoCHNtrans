	PRINTFORMW 「…捨てたり、しないでね？」
	CFLAG:5001 = 5
	RETURN 1
	
;-------------------------------------------------
;助手との掛け合い
;-------------------------------------------------
ELSEIF ASSI > 0 && NO:ASSI == 37
	DRAWLINE
	;初めて
