@K42_KOJO_MESSAGE_A
IF CFLAG:6950 == 0
	TRYCALLFORM SYSTXT_TRAIN_MESSAGE_F_COM_{SELECTCOM}
	WAIT
ELSEIF CFLAG:6950 == 1
	CFLAG:6950 = 0
ENDIF

@K42_KOJO_MESSAGE_B
;口上を表示するかの判定
;一回リセット
	CFLAG:6951 = 0
;助手が調教した時に口上をスキップする
IF ASSI > 0 && ASSIPLAY
	;特定コマンドは該当しない
	;二人フェラチオ、二人でパイズリ、二人で素股
	IF SELECTCOM == 303 || SELECTCOM == 305 || SELECTCOM == 306
		CFLAG:6951 = 0
	ELSE
		CFLAG:6951 = 1
	ENDIF
ENDIF
;眠ってる場合は口上をスキップする
SIF EQUIP:14
	CFLAG:6951 = 1
;口枷がある場合は口上をスキップする
SIF TEQUIP:42 >= 1 && (SELECTCOM != 226 && SELECTCOM != 229 && SELECTCOM != 506 && SELECTCOM != 509 && SELECTCOM != 583)
	CFLAG:6951 = 1
;触手貫通中は口上をスキップする
SIF TEQUIP:25 == 3 && SELECTCOM != 524
	CFLAG:6951 = 1
;触手強制回復中は口上をスキップする
SIF TEQUIP:91 >= 1 && SELECTCOM != 560
	CFLAG:6951 = 1
;気絶中も口上をスキップする
SIF TFLAG:899 >= 1
	CFLAG:6951 = 1


;装備品口上呼び出し
	CALL KOJO_X_TRAIN_MESSAGE_EQUIPTEXT


;特殊な精神状態における口上呼び出し
;狂気または精神崩壊
IF (TALENT:狂気 == 1 || TALENT:精神崩壊 == 1) && CFLAG:6951 == 0
	CFLAG:6951 = 1
	CALL K42_MIND_BREAK_A
;幼児退行
ELSEIF TALENT:幼児退行 == 1 && CFLAG:6951 == 0
	CFLAG:6951 = 1
	CALL K42_MIND_BREAK_B
ENDIF


;コマンド実行後の地の文表示
@K42_KOJO_MESSAGE_C
IF CFLAG:6950 == 0
	TRYCCALLFORM SYSTXT_TRAIN_TEXT_COM_{SELECTCOM}
	WAIT
	CATCH
	ENDCATCH
ELSEIF CFLAG:6950 == 1
	CFLAG:6950 = 0
ENDIF

