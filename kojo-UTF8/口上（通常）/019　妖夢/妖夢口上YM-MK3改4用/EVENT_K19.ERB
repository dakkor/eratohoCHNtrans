		IF CFLAG:6000 == 1 
			PRINTFORMW 「はぁ、いいお湯でした」
			CFLAG:8824 = 4
		;服従
		ELSEIF CFLAG:6000 == 2 
			PRINTFORMW 「妖夢の体を綺麗にしていただいてありがとうございます、%STR:401%」
			CFLAG:8824 = 3
		;それ以外
		ELSE
			PRINTFORMW 「これで今日は終わってもらえると、もっと嬉しいのですが……」
			CFLAG:8824 = 2
		ENDIF
			RETURN 0
	ENDIF
;装着時
ELSE
	;初めて
	IF CFLAG:TARGET:9350 == 0
		;恋慕
		IF CFLAG:6000 == 1
			PRINTFORMW 「い、一緒にお風呂に！？」
			PRINTFORMW 「その、男性と女性が一緒にお風呂に入るというのは……」
			PRINTFORMW 「いえ！　決して嫌という訳では！」
		;服従
		ELSEIF CFLAG:6000 == 2
			PRINTFORMW 「お風呂に、一緒に入ってくださるのですか？」
		;それ以外
		ELSE
			PRINTFORMW 「お風呂に入れるのは嬉しいですが……」
			PRINTFORMW 「……それだけではないのですよね？」
		ENDIF
			CFLAG:TARGET:9350 = 1
			RETURN 0
	;二回目以降
	ELSE
		;恋慕
		IF CFLAG:6000 == 1 
			PRINTFORMW 「あ、またお風呂に行くのですね」
			CFLAG:9350 = 4
		;服従
		ELSEIF CFLAG:6000 == 2 
			PRINTFORMW 「またお風呂に連れて行ってくださるのですね」
			CFLAG:9350 = 3
		;それ以外
