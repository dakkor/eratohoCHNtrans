;処女喪失時のメッセージ呼び出し
SIF TALENT:処女 && TFLAG:1
	TRYCALL K19_MESSAGE_LOST_VIRGIN
;初めてPALAMがLV2超えた時の口上呼び出し
FOR LOCAL, 0, 35
	SIF PALAM:LOCAL + UP:LOCAL > PALAMLV:2 && CFLAG:(5320 + LOCAL) == 0
		TRYCALLFORM K19_MESSAGE_OVERLV2_{LOCAL}
NEXT
;絶頂時の口上呼び出し
