@KOJO_19_MESSAGE_IMPRESSION
;マスターの呼び方を決定
CALL K19_MASTERNAME
;体力消耗
SIF MAXBASE:0 > 0
	LOCAL:1 = (100 * BASE:0) / MAXBASE:0
;気力消耗
SIF MAXBASE:1 > 0
	LOCAL:2 = (100 * BASE:1) / MAXBASE:1
;酔い具合
SIF MAXBASE:8 > 0
	LOCAL:3 = (100 * BASE:8) / MAXBASE:8
;眠っている場合
IF EQUIP:14
	PRINTSINGLEFORM 　　　「（すやすや～～～）」
;精神崩壊
ELSEIF TALENT:167
	PRINTSINGLEFORM 　　　「……。」
;まだ一度も調教していない
ELSEIF CFLAG:5201 == 0
	PRINTSINGLEFORM 　　　「楼観剣も白楼剣も奪われてしまうとは……不覚……」
;調教済みで陥落前
ELSEIF CFLAG:6000 == 0
	LOCAL = RAND:3
	;体力消耗or気力消耗
	IF LOCAL == 0 && (LOCAL:1 <= 25 || LOCAL:2 <= 25)
		PRINTSINGLEFORM 　　　（今はとにかく休もう……）
	;情報表示
	ELSEIF LOCAL == 1
		LOCAL = RAND:4
		;従順
		IF LOCAL == 0
			IF ABL:従順 >= 3
				PRINTSINGLEFORM 　　　「わかりました、%STR:401%に従います……」
			ELSE
				PRINTSINGLEFORM 　　　「どうして%STR:401%の言う事を聞かなければならないのですか」
			ENDIF
		;欲望
		ELSEIF LOCAL == 1
			IF ABL:欲望 >= 3
				PRINTSINGLEFORM 　　　（あんなに気持ちいい事、我慢できない……）
			ELSE
				PRINTSINGLEFORM 　　　「私は欲望には流されませんよ」
			ENDIF
		;奉仕精神
		ELSEIF LOCAL == 2
			IF ABL:奉仕精神 >= 3
				PRINTSINGLEFORM 　　　「奉仕して悦んでもらえるのは、満更でもないです」
			ELSE
				PRINTSINGLEFORM 　　　「%STR:401%に奉仕するなんて嫌です」
			ENDIF
		;料理技能
		ELSEIF LOCAL == 3
			IF ABL:料理技能 >= 3
				PRINTSINGLEFORM 　　　「料理ぐらいならしてもいいですけど……」
			ELSE
				PRINTSINGLEFORM 　　　「どうして%STR:401%の為に料理をしなければならないのですか」
			ENDIF
		ENDIF
	;それ以外
	ELSE
		LOCAL = RAND:2
		;反発刻印Lv3
		IF MARK:反発刻印 == 3
			IF LOCAL == 0
				PRINTSINGLEFORM 　　　「いつか斬ってやるわ」
			ELSE
				PRINTSINGLEFORM 　　　「剣さえあれば%STR:401%なんかに……！」
			ENDIF
		;屈服刻印Lv3
		ELSEIF MARK:屈服刻印 == 3
			IF LOCAL == 0
				PRINTSINGLEFORM 　　　「もうここから出られないのかな……」
			ELSE
				PRINTSINGLEFORM 　　　（%STR:401%には逆らえないのね……）
			ENDIF
		;恐怖刻印Lv3
		ELSEIF MARK:恐怖刻印 == 3
			IF LOCAL == 0
				PRINTSINGLEFORM 　　　「逃げようとなんてしたら、何をされるか……」
			ELSE
				PRINTSINGLEFORM 　　　「もうあんな目に遭うのは嫌……」
			ENDIF
		;それ以外
		ELSE
			IF LOCAL == 0
				PRINTSINGLEFORM 　　　「どうにかして脱出しないと……」
			ELSE
				PRINTSINGLEFORM 　　　（幽々子さま……）
			ENDIF
		ENDIF
	ENDIF
;陥落済み
ELSEIF CFLAG:6000 > 0
	LOCAL = RAND:7
	;体力&気力
	IF LOCAL == 0
		;体力消耗or気力消耗
		IF LOCAL:1 <= 25 || LOCAL:2 <= 25
			;恋慕
			IF CFLAG:6000 == 1
