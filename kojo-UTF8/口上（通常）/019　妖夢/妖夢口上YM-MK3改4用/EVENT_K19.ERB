		ENDIF
	;素質
	ELSEIF LOCAL == 1
		LOCAL = RAND:4
		;相愛
		IF LOCAL == 0 && TALENT:相愛
			PRINTSINGLEFORM 　　　「%STR:401%がいつ来てもいいようにしておかないと……」
		;隷属
		ELSEIF LOCAL == 0 && TALENT:隷属
			PRINTSINGLEFORM 　　　「%STR:401%がいつでも私を使えるようにしておかないと……」
		;妄信
