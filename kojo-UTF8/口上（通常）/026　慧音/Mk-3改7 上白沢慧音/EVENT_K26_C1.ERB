;ビデオ撮影
;TEQUIP:50 == 0で解除口上
;-------------------------------------------------
@KOJO_26_TRAIN_MESSAGE_S_COM_100
;コマンド選択での撮影終了時
IF TEQUIP:50 == 0
	;PRINTFORMW 
;撮影開始時
ELSE
	;PRINTFORMW 
	;初めて
	IF CFLAG:8313 == 0
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORMW 「ビデオ…」
			PRINTFORMW 「教育者としては…こんな風俗、どうしたものか…」
		;その他の場合
		ELSE
			PRINTFORMW 「や、やめろ！　撮るな！」
			PRINTFORMW 「私だって…教育者の端くれなんだ！」
		ENDIF
		CFLAG:8313= 1
		RETURN 0
	;２回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORMW 「…１８歳未満の視聴、所持を禁ずる」
			PRINTFORMW カメラに向かって、それだけ言った。
			CFLAG:8313 = 3
		;その他の場合
		ELSE
			PRINTFORMW 「人の痴態なんて…撮るものじゃないだろう…」
			PRINTFORMW 撮られること自体は、既に諦めたようだ。
			CFLAG:8313 = 2
		ENDIF
		RETURN 0
	ENDIF
ENDIF
