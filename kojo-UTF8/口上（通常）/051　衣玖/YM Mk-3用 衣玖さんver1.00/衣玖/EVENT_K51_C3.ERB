			PRINTFORMW 「体を擦り合せてやるのですか…」
		ENDIF
		CFLAG:8440= 1
		RETURN 0
	;２回目以降
	ELSE
		;恋慕かつ奉仕精神Lv5
		IF TALENT:恋慕 == 1 && ABL:奉仕精神 >= 5 && (CFLAG:8440 <= 3)
			PRINTFORMW 「%CALLNAME:MASTER%様の体が…私の体に擦れて……私……もう」
			CFLAG:8440 = 4
		;奉仕精神Lv3
		ELSEIF ABL:奉仕精神 >= 3 && (CFLAG:8440 <= 2)
			PRINTFORMW 「んしょ、きゃあ！ちょっと下をおっ勃てないでください」
			CFLAG:8440 = 3
		;その他の場合
		ELSE
			PRINTFORMW 「はぁ、綺麗になるのはいいのですが疲れますね」
			CFLAG:8440 = 2
		ENDIF
		RETURN 0
	ENDIF

