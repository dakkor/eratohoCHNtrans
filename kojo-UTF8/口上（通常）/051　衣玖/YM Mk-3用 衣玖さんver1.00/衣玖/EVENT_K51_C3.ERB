;-------------------------------------------------
;お風呂場プレイ CFLAG:8438
;TEQUIP:53 == 0で解除口上
;-------------------------------------------------
@KOJO_51_TRAIN_MESSAGE_S_COM_350
;解除時
IF TEQUIP:53 == 0
	PRINTFORMW 「すっきりしました」
;装着時
ELSE
	;初めて
	IF CFLAG:8438 == 0
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORMW 「一緒にお風呂に入るのですね」
		;その他の場合
		ELSE
			PRINTFORML 「体を綺麗にですね」
			PRINTFORML 「……もしかして%CALLNAME:MASTER%と一緒に入るのですか？」
			PRINTFORMW 「はぁ、分かりました」
		ENDIF
		CFLAG:8438= 1
		RETURN 0
	;２回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1 && (CFLAG:8438 <= 2)
			PRINTFORMW 「綺麗にしましょうね」
			CFLAG:8438 = 3
		;その他の場合
		ELSE
			PRINTFORMW 「お風呂中妙な事しないでくださいね」
			CFLAG:8438 = 2
		ENDIF
		RETURN 0
	ENDIF
ENDIF

