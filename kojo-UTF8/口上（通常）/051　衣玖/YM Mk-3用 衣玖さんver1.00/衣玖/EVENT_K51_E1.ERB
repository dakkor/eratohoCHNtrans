;-------------------------------------------------
;弁当作成 CFLAG:7130
;TFLAG:99で料理の種類の番号を取得可能 
;-------------------------------------------------
@KOJO_51_MESSAGE_SUB_EVENT_SCENE400
;地の文呼び出し
TRYCALL SYSTXT_MESSAGE_SUB_EVENT_SCENE400
;初めて
	IF CFLAG:7130 == 0
		;料理得意修得イベント発生
		IF CFLAG:8001 == 1 && TALENT:恋慕 == 1 && TALENT:料理得意 == 0 && ABL:料理技能 >= 3 && EXP:調理経験 >= 100
			PRINTFORML 最近、%CALLNAME:TARGET%の料理の味がよくなってきている
			PRINTFORML 味だけではない手際もだ、最初のころに比べて料理が完成する時間が早くなってきている
			PRINTFORML どうやら数をこなしていくうちに腕が上がって慣れてきたようだ
			PRINTFORMW ---料理得意を習得しました---
			TALENT:料理得意 = 1
		;その他の場合
		ELSE
			PRINTFORMW 「料理ですって？一応作れますが味に期待しないでくださいね」
		ENDIF
		CFLAG:7130= 1
		RETURN 0
	;２回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1
			;料理技能Lv5以上
			IF ABL:料理技能 >= 5
				;乱数分岐:3
				IF RAND:3 == 0
					PRINTFORML 「%CALLNAME:MASTER%様…ちょっと待っていてくださいね…」
					PRINTFORML 今の私なら出来るかもしれない%CALLNAME:TARGET%はそう思った
					PRINTFORML 今なら…そう今なら…
					PRINTFORMW （豪華に見える超お手軽簡単手抜き料理を!!!）
				;乱数分岐:2
				ELSEIF RAND:2 == 0
					PRINTFORML 「これをこうして…あれを…」
					PRINTFORML 「よし、後はこの媚………」
					PRINTFORMW （うふふ、%CALLNAME:MASTER%様…今夜は愛し合いましょうね）
				;その他の場合
				ELSE
					PRINTFORMW 「%CALLNAME:MASTER%様の好きな物いっぱい作りましょう」
				ENDIF
			;料理技能Lv3以上
			ELSEIF ABL:料理技能 >= 3
				;乱数分岐:2
				IF RAND:2 == 0
					PRINTFORML 「パパッと作りますからね！」
					PRINTFORMW 「……痛っ、指を切ってしまいました」
				;その他の場合
				ELSE
					PRINTFORMW 「待っていてくださいね、すぐに出来ますから」
				ENDIF
			;その他の場合
			ELSE
				;乱数分岐:2
				IF RAND:2 == 0
					PRINTFORMW 「もっと上手になりたいものですね」
				;その他の場合
				ELSE
					PRINTFORMW 「%CALLNAME:MASTER%様の口に合うように頑張らなければ！」
				ENDIF
			ENDIF
		;その他の場合
		ELSE
			;料理技能Lv5以上
			IF ABL:料理技能 >= 5
				;乱数分岐:2
				IF RAND:2 == 0
					PRINTFORML 「美味しいものを食べれば%CALLNAME:MASTER%の心もきっと晴れる…」
					PRINTFORMW 「訳はないですよねぇ、はぁ」
				;その他の場合
				ELSE
					PRINTFORMW 「ここまで上手くなれればお店が開けそうですね」
				ENDIF
			;料理技能Lv3以上
			ELSEIF ABL:料理技能 >= 3
				;乱数分岐:2
				IF RAND:2 == 0
					PRINTFORMW 「さてと、何を作りましょうか」
				;その他の場合
				ELSE
					PRINTFORMW 「だんだん料理に慣れてきました」
				ENDIF
			;その他の場合
			ELSE
				;乱数分岐:2
				IF RAND:2 == 0
					PRINTFORMW 「どうして私、料理なんかしているのでしょう…」
				;その他の場合
				ELSE
					PRINTFORMW 「う～ん、ここは簡単な料理から始めましょう」
				ENDIF
			ENDIF
		ENDIF
		RETURN 0
	ENDIF

