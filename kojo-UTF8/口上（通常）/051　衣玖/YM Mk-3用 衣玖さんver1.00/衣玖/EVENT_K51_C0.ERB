;-------------------------------------------------
;裸エプロン CFLAG:82977
;TEQUIP:59 == 0で解除口上
;-------------------------------------------------
@KOJO_51_TRAIN_MESSAGE_S_COM_90
;解除時
IF TEQUIP:59 == 0
	;恋慕
	IF TALENT:恋慕 == 1
		PRINTFORMW 「もう脱いでしまっていいのですか？」
		RETURN 0
	;その他の場合
	ELSE
		PRINTFORMW 「結局脱がすのですね」
	ENDIF
;装着時
ELSE
	;初めて
	IF CFLAG:8277 == 0
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORML 「露出が減りましたけれどいいのでしょうか」
			PRINTFORMW 「男のロマン？………はぁ、よく分かりませんが着ますね」
		;その他の場合
		ELSE
			PRINTFORMW 「どうしてこんな物を着なければならないのでしょう」
		ENDIF
		CFLAG:8277= 1
		RETURN 0
	;２回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1 && (CFLAG:8277 <= 2)
			PRINTFORML 「%CALLNAME:MASTER%様、お食事になさいますか？ それともお風呂？ それともわ・た・し？」
			PRINTFORMW 「どうしたのです？これを言わせたくて着させたのでしょう」
			CFLAG:8277 = 3
		;その他の場合
		ELSE
			PRINTFORMW 「まぁ一応着ますけれど…」
			CFLAG:8277 = 2
		ENDIF
		RETURN 0
	ENDIF
ENDIF

