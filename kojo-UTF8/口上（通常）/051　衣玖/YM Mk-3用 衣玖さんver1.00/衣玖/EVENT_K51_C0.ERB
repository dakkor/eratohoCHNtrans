;==============================================================
;○○口上
;==============================================================
;========================================================
;コマンド口上
;CFLAG:9000～9099までの100個を使用して、
;選んだコマンドを対象に初めて使ったかどうかを判別できる
;初回かどうかは最初に@COM_FLAGを呼び出した上で
;RESULT:1が1かどうかで判別できる
;========================================================
;-------------------------------------------------
;愛撫 CFLAG:8200
;-------------------------------------------------
;========================================================
;コマンド口上前の共通関数
;特定の条件下でコマンド口上を表示させないようにする場合はRETURN 1
;1以外を返した場合はコマンド口上は表示される
;========================================================
@KOJO_51_TRAIN_MESSAGE_S_BEFORE

@KOJO_51_TRAIN_MESSAGE_S_COM_0
 ;地の文呼び出し
TRYCALL SYSTXT_TRAIN_MESSAGE_F_COM_0
	;初めて
	IF CFLAG:8200 == 0
		;恋慕
		IF TALENT:恋慕 == 1
			;助手が天子で助手が調教
			IF ASSIPLAY && NO:ASSI == 52
				;助手が恋慕である
				IF TALENT:ASSI:恋慕 == 1
					PRINTFORML 「うんっ、あぁっ！もっと私に触れてください」
					PRINTFORMW 『ん～、こうかしら？』
				;その他の場合
				ELSE
					PRINTFORML 『あんたこういう事がおかしいと思わないの？』
					PRINTFORML 「何を言っているのです？%CALLNAME:MASTER%様なら大丈夫ですよ。きっと総領娘様も気に入るはずです」
					PRINTFORMW 『もうすっかり調教されちゃってさ～～』
				ENDIF
			;その他の場合
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%様、手つきが厭らしいですよ」
			ENDIF
		;助手が天子で助手が調教
		ELSEIF ASSIPLAY && NO:ASSI == 52
			;助手が恋慕である
			IF TALENT:ASSI:恋慕 == 1
				PRINTFORMW 『さ～てと、どうしようかな』
				PRINTFORMW 「あ、あのこんな事は……ひゃぁぁ」
			;その他の場合
			ELSE
				PRINTFORMW 『じゃあ始めるわね』
				PRINTFORMW 「どうぞお手柔らかに」
			ENDIF
		;屈服刻印Lv2
		ELSEIF MARK:屈服刻印 == 2
			PRINTFORMW 「な、何度触れても気持ちよくなど…」
		;その他の場合
		ELSE
			PRINTFORMW 「いやっ、そんな所触らないでください」
		ENDIF
		CFLAG:8200= 1
		RETURN 0
	;２回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1 && (CFLAG:8200 <= 5)
			;助手が天子で助手が調教
			IF ASSIPLAY && NO:ASSI == 52
				;助手が恋慕である
				IF TALENT:ASSI:恋慕 == 1
					PRINTFORML 『%CALLNAME:TARGET%ってスタイル良いよね、こうボン・キュ・ボンって感じで』
					PRINTFORML 「ふふ、総領娘さまの身体だってスレンダーで美しいですよ」
					PRINTFORMW 『そ、そうかな？んーそうだといいけど』
				;その他の場合
				ELSE
					PRINTFORML 「気持ちよくしてくださいね」
					PRINTFORMW 『どうして私がこんなことを……』
				ENDIF
			;乱数分岐:2
			ELSEIF RAND:2 == 0
				PRINTFORMW 「んぁ…体のすみずみまで撫でまわしてください」
			;その他の場合
			ELSE
				PRINTFORMW 「…あんっ、%CALLNAME:MASTER%様の手…凄くいいです」
			ENDIF
			CFLAG:8200 = 6
		;助手が天子で助手が調教
		ELSEIF ASSIPLAY && NO:ASSI == 52 && (CFLAG:8200 <= 4)
			;助手が恋慕である
			IF TALENT:ASSI:恋慕 == 1
				PRINTFORML 「んっ、くぁっ、んんっ」
				PRINTFORML 『随分感じているみたいね』
				PRINTFORMW 「そ、そんなことは！ああっ」
			;その他の場合
			ELSE
				PRINTFORML 『……こんなのでどう？』
				PRINTFORML 「どう？と言われましても……複雑な気分です」
				PRINTFORMW 『奇遇ね、私もよ』
			ENDIF
			CFLAG:8200 = 5
		;屈服刻印Lv3
		ELSEIF MARK:屈服刻印 == 3 && (CFLAG:8200 <= 3)
			;乱数分岐:2
			IF RAND:2 == 0
				PRINTFORML 「ああんっ!!……はっ！」
				PRINTFORMW 「ひ、卑怯です！そんな所ばかり触るなんて…」
			;その他の場合
			ELSE
				PRINTFORML 「んぁ！ああ…はぁ…」
				PRINTFORMW 「手つきが厭らしいですよぉ」
			ENDIF
			CFLAG:8200 = 4
		;屈服刻印Lv2
		ELSEIF MARK:屈服刻印 == 2 && (CFLAG:8200 <= 2)
			;乱数分岐:2
			IF RAND:2 == 0
				PRINTFORMW 「んっ…そこはっ…」
			;その他の場合
			ELSE
				PRINTFORMW 「んっ、くっ……んんっ！」
			ENDIF
			CFLAG:8200 = 3
		;その他の場合
		ELSE
			;乱数分岐:2
			IF RAND:2 == 0
				PRINTFORMW 「駄目です！触らないでください」
			;その他の場合
			ELSE
				PRINTFORMW 「もうやめてください」
			ENDIF
			CFLAG:8200 = 2
		ENDIF
		RETURN 0
	ENDIF

