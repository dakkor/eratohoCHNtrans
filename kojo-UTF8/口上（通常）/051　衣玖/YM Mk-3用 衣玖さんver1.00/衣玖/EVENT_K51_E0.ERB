;-------------------------------------------------
@KOJO_51_MESSAGE_SUB_EVENT_SCENE330
;地の文呼び出し
TRYCALL SYSTXT_MESSAGE_SUB_EVENT_SCENE330
	;初めて
	IF CFLAG:7103 == 0
		;恋慕
		IF TALENT:恋慕 == 1
			PRINTFORML 「それを飲めば処女膜が再生するのですね」
			PRINTFORMW 「うふふ、再び%CALLNAME:MASTER%にあげますね」
		;その他の場合
		ELSE
			PRINTFORMW 「私の初めてを奪っておきながら何故そんな物飲ませるのですか」
		ENDIF
		CFLAG:7103= 1
		RETURN 0
	;２回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕 == 1 && (CFLAG:7103 <= 2)
			PRINTFORML 「%CALLNAME:MASTER%様も物好きですね」
			PRINTFORMW 「いいですよ、何度でも差し上げます」
			CFLAG:7103 = 3
		;その他の場合
		ELSE
			PRINTFORMW 「またその薬…%CALLNAME:MASTER%の考えている事が分かりませんよ」
			CFLAG:7103 = 2
		ENDIF
		RETURN 0
	ENDIF
	
