	$INPUT_BSIZE_LOOP
	INPUT
	IF RESULT == 0
		PRINTFORMW ―妹紅を貧乳にしました―
		TALENT:爆乳 = 0
		TALENT:巨乳 = 0
		TALENT:絶壁 = 0
		TALENT:貧乳 = 1
		;でも輝夜が貧乳に設定されてるのに妹紅が貧乳に設定されてないのはおかしいと思います！
	ELSEIF RESULT == 1
		PRINTFORMW ―妹紅を絶壁にしました―
		TALENT:爆乳 = 0
		TALENT:巨乳 = 0
		TALENT:貧乳 = 0
		TALENT:絶壁 = 1
	ELSEIF RESULT == 2
		PRINTFORMW ―通常モードで調教を開始します―
	ELSE
		GOTO INPUT_BSIZE_LOOP
	ENDIF
	PRINTL 妹紅は男性を惹きつけることに関して今の自分を……
	PRINTL [0] 輝夜より劣っていると「思い込んで」いる(イベントオン+初期素質に[魅力]追加)
	PRINTL [1] 別に何か思うことがあるわけでもない（そのまま、イベントもオフ）
	$INPUT_EVENT_LOOP
	INPUT
	IF RESULT == 0
		PRINTL ―[魅力]を追加―
		PRINTW ―イベントをオンにしました―
		CFLAG:6000 = 1
		TALENT:魅力 = 1
		;本人が勝手にかわいくとないと思っているだけで実際は可愛い、というわけ
		;流石に謎の魅力には及ばないが、しかし魅惑習得イベントあり
		SIF EXP:調理経験 < 4
			EXP:調理経験 = 4
		;花嫁修業をやってみて諦めた形跡（焼き鳥屋ではない）
		PRINTL イベントによる能力変化の範囲をどうしますか？
		PRINTL [0] 妹紅以外も
		PRINTL [1] 妹紅のみ
		$INPUT_EVENT_LOOP2
		INPUT
		IF RESULT == 0
			PRINTFORMW ―能力変化を全てオンにしました―
			CFLAG:7000 = 1
		ELSEIF RESULT == 1
			PRINTFORMW ―能力変化を一部を除いてオンにしました―
		ELSE
			GOTO INPUT_EVENT_LOOP2
		ENDIF
	ELSEIF RESULT == 1
		PRINTFORMW ―通常モードで調教を開始します―
	ELSE
		GOTO INPUT_EVENT_LOOP
	ENDIF
	PRINTL 妹紅と輝夜がやってるのは……
	PRINTL [0] 殺し愛だよ（相性上昇）
	PRINTL [1] 殺し合いだよ（そのまま）
	$INPUT_MOKOTERULOOP
	INPUT
	IF RESULT == 0
		IF RELATION:30 < 140
			PRINTW ―輝夜との相性を上げました―
			RELATION:30 = 140
		ELSE
			PRINTFORMW ―もう十分相性が上がっています―
		ENDIF
	ELSEIF RESULT == 1
		PRINTFORMW ―通常モードで調教を開始します―
	ELSE
		GOTO INPUT_MOKOTERULOOP
	ENDIF
	CFLAG:5020 = 1
ENDIF

