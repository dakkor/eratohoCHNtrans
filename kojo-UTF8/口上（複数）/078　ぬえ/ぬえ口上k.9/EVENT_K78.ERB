;エキストラモード構文　CFLAG:8001を使用
;はいを選択した時に初調教時に口上設定用の素質に変更できます
;下記に処女化する場合の構文をサンプルとして載せておきます
;-------------------------------------------------;
@KOJO_78_EXTRA_INITIALIZATION
IF CFLAG:8001 == 0
	PRINTL 処女モードに移行しますか？
	PRINTL [0] はい
	PRINTL [1] いいえ
	$INPUT_LOOP0
	INPUT
	;はい
	IF RESULT == 0
		PRINTFORMW 処女追加　V感およびV経験0になりました
		TALENT:0 = 1
		EXP:Ｖ経験 = 0
		ABL:Ｖ感覚 = 0
		CFLAG:8001 = 1
	;いいえ
	ELSEIF RESULT == 1
		PRINTFORMW 何も変動しません
		CFLAG:8001 = 2
	ELSE
		GOTO INPUT_LOOP0
	ENDIF
;		コンボプレイは私には無理だった、でも名残だけ置いておく
;		CFLAG:5220 == 0
;			PRINTL この口上はコンボプレイが入っています
;			PRINTL この機能を使えるようにしますか？
;			PRINTL [0] ええ
;			PRINTL [1] だめ
;			$INPUT_LOOP1
;			INPUT
;			IF RESULT == 0
;				PRINTFORMW 特定の組み合わせで変わるかも？
;				CFLAG:5220 = 2
;			ELSEIF RESULT == 1
;				PRINTFORMW たぶん何もおきない・・・はず
;			ELSE
;				GOTO INPUT_LOOP1
;		ENDIF
	PRINTL 霊夢との相性が上がりますがどうします？
	PRINTL [0] とりあえず上げとく
	PRINTL [1] そのまま
	$INPUT_LOOP2
	INPUT
	IF RESULT == 0
		PRINTW 霊夢との相性が上がりました
		PRINTFORMW 
		PRINTFORMW 
		RELATION:1 = 120
	ELSEIF RESULT == 1
		PRINTFORMW 特に変更しません
		PRINTFORMW 
		PRINTFORMW 
	ELSE
		CFLAG:5220 = 1
		GOTO INPUT_LOOP2
	ENDIF
ENDIF

