;下の処理において、5303というのは例で実際は口上で許された空いているCFLAGを使ってください
;なお、専用日常イベントに関してもエキストラ構文のようにして使うかどうかを選ばせるといいでしょう
;専用日常イベントを許可していないなら戻る(キャラ別)
SIF CFLAG:ARG:7900 == 1
	RETURN 0

;フラグリセット(必ずお願いします)
CFLAG:ARG:406 = 0

;各個イベント分岐部
;基本、EX専用
IF CFLAG:ARG:0 == 1
	;専用日常がはじめてならばイベント発生可否を問う
	IF CFLAG:ARG:7900 == 0
		CALL DAILY_3_PERMISSION_CHECK, ARG
		;イベント発生を許可しないなら戻る
		SIF CFLAG:ARG:7900 == 1
			RETURN 0
	ENDIF
	;イベント本体呼び出し　一度見たイベントは起きないようにする
	;昼で、恋慕で、反発恐怖無し
	IF CFLAG:ARG:7901 == 0 && TIME == 0 && TALENT:ARG:恋慕 && MARK:ARG:反発刻印 == 0 && MARK:ARG:恐怖刻印 == 0
		CALL DAILY_EVENT_3_BOOK1, ARG
		CFLAG:ARG:7901 = 1
		;フラグセット(これで専用イベント直後に別途日常イベントが起きることはなくなる)
		CFLAG:ARG:406 = 1
	ELSEIF CFLAG:ARG:7901 == 1 && TIME == 0 && TALENT:ARG:恋慕 && MARK:ARG:反発刻印 == 0 && MARK:ARG:恐怖刻印 == 0
		CALL DAILY_EVENT_3_BOOK2, ARG
		CFLAG:ARG:7901 = 2
		;フラグセット(これで専用イベント直後に別途日常イベントが起きることはなくなる)
		CFLAG:ARG:406 = 1

	ENDIF
	RETURN 0
ENDIF

