		IF TEQUIP:PLAYER:31 == 0
			;PRINTFORMW 
		;装着時
		ELSE
			;PRINTFORMW 
		ENDIF
	CASE 1
		;解除
		IF TEQUIP:31 == 0
			;PRINTFORMW 
		;装着時
		ELSE
			;PRINTFORMW 
		ENDIF
	CASE 2
		;解除
